<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_for @user do |f| %>
  <div>
    <%= f.label :email%>
    <%= f.text_field :email%>
  </div>
	<div>
    <%= f.label :password%>
    <%= f.password_field :password%>
  </div>
  <div>
    <%= f.label :password_confirmation%>
    <%= f.password_field :password_confirmation%>
  </div>
  <%= f.submit "登録"%>
<% end %>
