<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_tag login_path, method: :post do %>
  <div>
    <%= label_tag :email%>
    <%= text_field_tag :email%>
  </div>
	<div>
    <%= label_tag :password%>
    <%= password_field_tag :password%>
  </div>
  <%= submit_tag "Login"%>
<% end %>
<%= link_to('新規登録はこちら', new_user_path)%>
